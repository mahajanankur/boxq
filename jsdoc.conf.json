{
  "source": {
    "include": ["./src/"],
    "includePattern": "\\.(js|ts)$",
    "excludePattern": "(node_modules/|tests/|dist/)"
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "static": true,
    "sort": true,
    "sectionOrder": [
      "Tutorials",
      "Classes",
      "Modules",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Interfaces"
    ]
  }
}
