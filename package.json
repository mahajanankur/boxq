{
  "name": "boxq",
  "version": "1.0.0",
  "description": "BoxQ - The ultimate SQS library for Node.js. Enterprise-grade reliability with circuit breaker, retry logic, and comprehensive monitoring.",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest tests/integration/real-aws.test.js",
    "test:all": "npm test && npm run test:integration",
    "test:e2e": "node tests/test-end-to-end.js",
    "test:long-polling": "node tests/test-long-polling.js",
    "test:microservices": "node tests/test-microservices-simple.js",
    "test:integration-runner": "node tests/run-integration-test.js",
    "test:workflows": "node scripts/test-workflows.js",
    "example:microservice": "node examples/express-microservice.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "docs": "jsdoc -c jsdoc.conf.json",
    "prepublishOnly": "npm run test:coverage",
    "dev": "nodemon src/index.js"
  },
  "keywords": [
    "sqs",
    "aws",
    "queue",
    "boxq",
    "messaging",
    "enterprise",
    "circuit-breaker",
    "fifo",
    "monitoring",
    "production",
    "nodejs",
    "microservices",
    "event-driven",
    "reliable",
    "scalable",
    "retry",
    "batch",
    "parallel",
    "sequential",
    "health-check",
    "metrics",
    "deduplication",
    "throttling"
  ],
  "author": "Ankur Mahajan",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.450.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "docdash": "^2.0.2",
    "dotenv": "^17.2.3",
    "eslint": "^8.53.0",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mahajanankur/boxq.git"
  },
  "bugs": {
    "url": "https://github.com/mahajanankur/boxq/issues"
  },
  "homepage": "https://github.com/mahajanankur/boxq#readme"
}
